{
  "name": "@sendspin/sendspin-js",
  "version": "0.1.0",
  "type": "module",
  "description": "TypeScript client library for the Sendspin synchronized audio protocol",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "node scripts/bundle-silent-audio.js",
    "build": "tsc",
    "dev": "tsc --watch",
    "dev-server": "rm -rf public/dev && concurrently \"tsc --watch\" \"rollup -c --watch\" \"serve . -l 6001\"",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "prepare": "husky",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "audio",
    "streaming",
    "music-assistant"
  ],
  "license": "Apache-2.0",
  "author": "Sendspin Protocol Authors <sendspin@openhomefoundation.org>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sendspin/sendspin-js.git"
  },
  "homepage": "https://github.com/Sendspin/sendspin-js#readme",
  "bugs": {
    "url": "https://github.com/Sendspin/sendspin-js/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@typescript-eslint/eslint-plugin": "7.0.0",
    "@typescript-eslint/parser": "6.21.0",
    "concurrently": "^9.2.1",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "husky": "^9.1.7",
    "prettier": "3.8.1",
    "rollup": "^4.54.0",
    "serve": "^14.2.5",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "opus-encdec": "^0.1.1"
  }
}
